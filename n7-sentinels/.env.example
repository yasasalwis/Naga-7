# General
ENVIRONMENT=development
LOG_LEVEL=INFO

# NATS
NATS_URL=nats://localhost:4222
NATS_CLUSTER_ID=n7-cluster

# Agent Identity
AGENT_TYPE=sentinel
AGENT_SUBTYPE=endpoint
AGENT_ID=sentinel-1
ZONE=default

# Core API
CORE_API_URL=http://localhost:8000/api/v1

# Deception Engine
DECEPTION_ENABLED=True
DECEPTION_DECOY_DIR=/tmp/n7_decoys

# Authentication
API_KEY_FILE=.agent_api_key
